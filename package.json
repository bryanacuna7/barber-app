{
  "name": "barberapp",
  "version": "0.9.6",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:skip-ts": "SKIP_TYPE_CHECK=true next build",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:security": "bash scripts/generate-security-report.sh",
    "test:security:watch": "vitest --reporter=verbose \"**/*.security.test.ts\"",
    "test:integration": "vitest run --reporter=verbose \"**/*.integration.test.ts\"",
    "test:hooks": "vitest run src/hooks/__tests__",
    "test:components": "vitest run \"src/components/**/__tests__/*.test.tsx\"",
    "test:api": "vitest run \"src/app/api/**/__tests__/*.test.ts\"",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:report": "vitest run --reporter=html",
    "test:report:json": "vitest run --reporter=json --outputFile=test-results.json",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:trace": "bash scripts/open-latest-trace.sh",
    "test:e2e:mi-dia": "playwright test tests/e2e/mi-dia.spec.ts",
    "test:mobile": "playwright test --project=mobile-360 --project=mobile-375 --project=mobile-390 --project=mobile-412 --project=mobile-320 --project=mobile-375-tall --project=mobile-393 --project=mobile-414 --project=mobile-428 --project=mobile-430",
    "test:mobile:tier1": "playwright test --project=mobile-360 --project=mobile-375 --project=mobile-390 --project=mobile-412",
    "test:mobile:tier2": "playwright test --project=mobile-320 --project=mobile-375-tall --project=mobile-393 --project=mobile-414 --project=mobile-428 --project=mobile-430",
    "test:mobile:tablet": "playwright test --project=tablet-768 --project=tablet-810",
    "explore": "bash scripts/explore-ui.sh",
    "explore:competitor": "bash scripts/compare-competitor.sh",
    "record:flow": "bash scripts/record-user-flow.sh",
    "test:performance": "lighthouse http://localhost:3000/mi-dia --only-categories=performance --output=html --output-path=./performance-report.html",
    "test:all": "npm run test:security && npm run test:unit && npm run test:integration && npm run test:e2e",
    "deploy:preview": "bash scripts/deploy-with-push.sh",
    "deploy:prod": "bash scripts/deploy-with-push.sh --prod",
    "validate:changelog": "node scripts/validate-public-changelog.mjs",
    "ci": "npm run validate:changelog && npm run format:check && npm run test:unit && npm run build:skip-ts",
    "ci:full": "npm run ci && npm run test:e2e",
    "ci:lint": "npm run lint",
    "ci:security": "npm run test:security"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@react-email/components": "^1.0.6",
    "@sentry/nextjs": "^10.38.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.91.1",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@upstash/redis": "^1.36.2",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "framer-motion": "^12.29.0",
    "fuse.js": "^7.1.0",
    "lucide-react": "^0.563.0",
    "next": "^16.1.6",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-email": "^5.2.5",
    "recharts": "^3.7.0",
    "resend": "^6.9.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "web-push": "^3.6.7",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
    "@next/bundle-analyzer": "^16.1.6",
    "@playwright/test": "^1.58.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/web-push": "^3.6.4",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.18",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jsdom": "^27.4.0",
    "lint-staged": "^16.2.7",
    "msw": "^2.12.8",
    "playwright": "^1.58.0",
    "prettier": "^3.8.1",
    "supabase": "^2.72.8",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.18"
  },
  "description": "This is a [Next.js](https://nextjs.org) project bootstrapped with [`create-next-app`](https://nextjs.org/docs/app/api-reference/cli/create-next-app).",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
